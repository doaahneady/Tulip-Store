      <style>
        #main-view {
          display: inline;
        }
        #products-cards {
          display: none;
        }
        /* card explore */
        .cards {
          display: flex;
          justify-content: center;
          align-items: flex-start;
          flex-wrap: wrap;
          margin-bottom: 10px;
        }

        .card-product {
          background-color: #ffffff67;
          width: 430px;
          height: 420px;
          border-radius: 20px;
          margin: 5px;
          padding: 15px;
          box-shadow: 0 5px 20px rgba(0, 0, 0, 0.3);
          overflow: hidden;
          display: flex;
          flex-direction: column;
        }

        .card-grid {
          display: grid;
          grid-template-columns: repeat(2, 1fr);
          gap: 8px;
          height: 90%;
        }

        .card-grid .figure {
          margin: 0;
          border-radius: 10px;
          overflow: hidden;
          position: relative;
          width: 100%;
          height: 90%;
        }

        .card-grid img {
          width: 100%;
          height: 100%;
          object-fit: cover;
          transition: transform 0.3s ease;
        }

        .card-grid img:hover {
          transform: scale(1.05);
        }

        .card-grid:has(.figure:nth-child(1):last-child) .figure {
          grid-column: span 2;
          grid-row: span 2;
        }

        .card-grid:has(.figure:nth-child(2):last-child) .figure {
          grid-row: span 1;
          height: 100%;
        }

        .card-grid:has(.figure:nth-child(3):last-child) .figure:nth-child(3) {
          grid-column: span 2;
        }
        /* ***************************************************************************************** */

        /* side bar */
        .filter {
          /* margin-top: 20px; */
          border-right: 2px solid rgba(0, 0, 0, 0.3);
          /* box-shadow: 0 5px 20px rgba(0, 0, 0, 0.3); */
        }
        .list-group {
          background-color: rgb(207, 207, 207);
          padding: 10px;
          margin: 4px;
          list-style: none;
        }
        /* product */

        .header {
          width: 100%;
          text-align: center;
        }
        .header h1 {
          text-transform: uppercase;
          color: #f05928;
        }
        .products {
          display: grid;
          grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));
          gap: 20px;
          align-items: start;
        }

        .product {
          position: relative;
          justify-content: space-between;
          background-color: #ffffff;
          box-shadow: 0 5px 20px rgba(0, 0, 0, 0.3);
          width: 200px;
          min-width: 230px;
          height: 340px;
          padding: 10px 10px 30px;
          border-radius: 10px;
          transition: 0.3s;
        }
        .product:hover {
          transform: translateY(-15px);
          box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
        }
        .image {
          width: 100%;
          height: 180px;
          display: grid;
          place-items: center;
        }
        .image img {
          border-radius: 10px;
          width: 100%;
          height: 100%;
          object-fit: cover;
          display: flex;
          justify-content: space-between;
        }
        .name_price {
          /* padding: 10px 0; */
          font-size: 15px;
        }
        .name_price h3 {
          font-size: 20px;
          font-weight: bold;
          text-transform: capitalize;
          color: #0d464c;
        }
        .name_price span {
          font-size: 14px;
          font-weight: bold;
          color: #f05928;
        }
        .product p {
          font-size: 12px;
        }
        .stars .fa-star {
          font-size: 1em;
          color: #f05928;
          background-color: #0d464c00;
          border: none;
        }
        .add-cart {
          position: absolute;
          right: 20px;
        }

        .add-cart button {
          padding: 5px 10px;
          border-radius: 7px;
          border: none;
          font-size: 14px;
          text-transform: capitalize;
          cursor: pointer;
          transition: 0.5s;
          box-shadow: 0 5px 20px rgba(0, 0, 0, 0.427);
          background-color: #b5b9bac9;
          color: #0d464c;
        }
        .add-cart button:hover {
          color: white;
          background-color: #0d464c;
        }
        .add-cart button:focus {
          color: white;
          background-color: #0d464c;
        }

        /* color checkboxes */
        .color-filter {
          margin-top: 8px;
          display: flex;
          flex-direction: column;
          gap: 2px;
        }
        .color-input {
          position: absolute;
          opacity: 0;
          pointer-events: none;
        }

        .swatch {
          width: 22px;
          height: 22px;
          border-radius: 50%;
          border: 1px solid rgba(0, 0, 0, 0.15);
          box-sizing: border-box;
          display: inline-block;
          flex: 0 0 22px;
        }

        /* show selected state */
        .color-input:checked + .color-label .swatch {
          box-shadow: 0 0 0 3px #000000cc,
            inset 0 0 0 1px rgba(255, 255, 255, 0.781);
          transform: scale(0.92);
        }

        /* show a checkmark on darker swatches for clarity */
        .color-input:checked + .color-label .swatch::after {
          content: "";
          display: block;
          text-align: center;
          line-height: 22px;
          font-size: 12px;
          color: white;
        }
        /* white swatch checkmark color override */
        .color-input:checked + .color-label .swatch.white::after {
          color: #0d464c;
        }
      </style>
    </main>

    <!-- footer -->
    <%- include('./includes/footer') %>

    <!-- sign in modal -->

    <!-- cart modal -->
    <%- include('./includes/cart') %>

    <!--  -->

    <script>
      document.addEventListener("DOMContentLoaded", function () {
        const product_view = document.getElementById("product-view");
        const main_view = document.getElementById("main-view");
        const products_main = document.getElementById("products-cards");

        product_view.addEventListener("click", function () {
          products_main.style.display = "grid";
          products_main.style.gridTemplateColumns = "20% auto";
          products_main.style.gap = "20px";
          main_view.style.display = "none";
        });
      });
    </script>
  </body>
</html>
